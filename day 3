#include <stdio.h>

int main()
{
  int a,b,sum;
  printf("\nEnter 2 numbers: ");
  scanf("%d%d",&a,&b);
  sum=a+b;
  printf("\nSum of 2 numbers is= %d",sum);
  return 0;
}

                                                                                                                                    
  Enter 2 numbers: 11                                                                                                                  
9                                                                                                                                    
                                                                                                                                     
Sum of 2 numbers is= 20
