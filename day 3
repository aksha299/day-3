int biggest(num1,num2)
{
  if(num1==num2)
   printf("numbers are equal.");
  else if(num1>num2)
   printf("first number you entered is greater than the second number you entered");
  else 
   printf("second number you entered is greater than the first number you entered");
  return 0;
 }
  \\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\\
  int big(int num1, int num2,int num3)
  {
   if(num1>num2)
   {
     if(num1>num3)
     {
      printf("number1 is greater");
      return 0;
     }
     else
     {
      printf("number3 is greater");
      return 0;
     }
   }
  else if(num2>num1)
  {
    if(num2>num3)
    {
      printf("number2 is greater");
      return 0;
    }
    else
    {
      printf(number3 is greater");
      return 0;
    }
  }
 }
 
    
